package main;

public class TriangleNumbers {
	public static void main(String[] args) {
		long trinum = 0;
		for(int i=0;/*divisorNum(trinum)*/i<500;i++){
			//System.out.println("i: " + i);
			
			trinum += i;
			System.out.println(trinum);
		}
		System.out.println("Final: "+trinum);
	}
	public static int divisorNum(long num){
		int count = 0;
		for(int i=2;i<=num;i++){
			if(num%i==0){
				count++;
			}
		}
		return count;	
	}

}
